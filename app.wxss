/**iconfont**/
@font-face {
  font-family: 'iconfont';  /* project id 1359249 */
  src: url('//at.alicdn.com/t/font_1359249_llave0pf7x.eot');
  src: url('//at.alicdn.com/t/font_1359249_llave0pf7x.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1359249_llave0pf7x.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1359249_llave0pf7x.woff') format('woff'),
  url('//at.alicdn.com/t/font_1359249_llave0pf7x.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1359249_llave0pf7x.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

